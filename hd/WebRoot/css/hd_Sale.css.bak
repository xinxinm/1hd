.box {
	position:relative;
	width:900px;
	height:350px;
	overflow:hidden;
	float:left;
	margin-top:10px;
}

.box ul.img {
	position:absolute;
	left:0;
	width:2700px;
	float:left;
	display:block;
}

.box  ul.img li {
	float:left;
}

.box  ul.icon {
	position:absolute;
	bottom:0;
	left:0;
	width:100%;
	text-align:center;
}

.box ul.icon  li {
	width:30px;
	height:4px;
	background:#DDDDDD;
	display:inline-block;
}

.box ul.icon  li p {
	width:0px;
	height:4px;
	background:red;
}

